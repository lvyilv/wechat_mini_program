/* pages/scroll/scroll.wxss */
.container1{
    
    border:1px solid red;
    /* display: flex; */
    height: 130px;
    /* width: 120px; */
    
}
.container1 view {
    width: 100px;
    height: 100px;
    text-align: center;
    line-height: 100px;
}
.container1 view:nth-child(1) {
    background-color: aqua;
}

.container1 view:nth-child(2) {
    background-color: yellow;
}

.container1 view:nth-child(3) {
    background-color: rebeccapurple;
}